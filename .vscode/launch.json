{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/apps/client/app/battle/Lobby.tsx",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node-terminal",
      "name": "Run Script: dev",
      "request": "launch",
      "command": "yarn dev",
      "cwd": "${workspaceFolder}/apps/client"
    }
  ]
}
